<?xml version="1.0" encoding="UTF-8" ?>

<conditions xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

	<!-- Basic: -->
	<where subject="?name" rel="startswith" value="Fred"/>
	<where subject="Freddy" rel="startswith" value="?name"/>
	<where subject="?name" rel="startswith" value="?fragment"/>

	<!-- Same as above, but negated: -->
	<where subject="?name" rel="startswith" value="Fred" negate="true" />
	<where subject="Freddy" rel="startswith" value="?name" negate="true" />
	<where subject="?name" rel="startswith" value="?fragment" negate="true" />

	<!-- Case-sensitivity: -->
	<where subject="?name" rel="startswith" value="Fred" ignorecase="true" />
	<where subject="Freddy" rel="startswith" value="?name" ignorecase="true"/>
	<where subject="?name" rel="startswith" value="?fragment" ignorecase="true"/>

	<!-- Multiples: -->
	<where subject="?name" rel="startswith" value="Fred,Sam,Jo" multiple="true" />
	<where subject="?name" rel="startswith" value="Fred,Sam,Jo" multiple="true" negate="true" />
	<where subject="Freddy,Samantha,Joseph" rel="startswith" value="?fragment" multiple="true" />
	<where subject="Freddy,Samantha,Joseph" rel="startswith" value="?fragment" multiple="true" negate="true" />
	<where subject="?name1,?name2,?name3" rel="startswith" value="?fragment" multiple="true" />
	<where subject="?name1,?name2,?name3" rel="startswith" value="?fragment" multiple="true" negate="true" />

</conditions>